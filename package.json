{
  "name": "cloud-spirit-cultivation",
  "private": true,
  "version": "0.3.1.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && npx gh-pages -d dist",
    "vercel-build": "npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "docker:build-image": "node scripts/docker-build.js build-image",
    "docker:build": "node scripts/docker-build.js build",
    "docker:build-no-cache": "node scripts/docker-build.js build-no-cache",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:pack": "node scripts/docker-build.js pack",
    "docker:pack-uncompressed": "node scripts/docker-build.js pack-uncompressed",
    "docker:build-and-pack": "npm run docker:build-image && npm run docker:pack-uncompressed",
    "docker:build-and-up": "node scripts/docker-build.js build-and-up",
    "docker:push-image": "node scripts/docker-build.js build-image && node scripts/docker-build.js push-image",
    "party:dev": "partykit dev",
    "party:deploy": "partykit deploy"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "dayjs": "^1.11.19",
    "lucide-react": "^0.554.0",
    "partykit": "^0.0.115",
    "partysocket": "^1.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "code-inspector-plugin": "^1.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.1",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
